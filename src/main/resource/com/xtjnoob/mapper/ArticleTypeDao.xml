<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.4//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xtjnoob.mapper.ArticleTypeDao">

    <resultMap id="resultMap" type="ArticleType">
        <result property="code" column="code" javaType="String"/>
        <result property="name" column="name" javaType="String"/>
        <result property="remark" column="remark" javaType="String"/>
    </resultMap>

    <select id="getAllArticleTypes" resultMap="resultMap">
        SELECT * FROM `article_type`
    </select>
</mapper>